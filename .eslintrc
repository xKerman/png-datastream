{
    "root": true,
    "extends": "eslint:recommended",
    "env": {
        "es6": true
    },
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module"
    },
    "rules": {
        "semi": 2,
        "no-extra-parens": 2,
        "no-extra-semi": 2,
        "accessor-pairs": 2,
        "array-callback-return": 2,
        "consistent-return": 2,
        "curly": 2,
        "default-case": 2,
        "eqeqeq": 2,
        "guard-for-in": 2,
        "no-alert": 2,
        "no-caller": 2,
        "no-div-regex": 2,
        "no-else-return": 2,
        "no-empty-function": 2,
        "no-eq-null": 2,
        "no-eval": 2,
        "no-extend-native": 2,
        "no-extra-bind": 2,
        "no-fallthrough": 2,
        "no-floating-decimal": 2,
        "no-implicit-globals": 2,
        "no-implied-eval": 2,
        "no-invalid-this": 2,
        "no-labels": 2,
        "no-lone-blocks": 2,
        "no-loop-func": 2,
        "no-param-reassign": 2,
        "no-return-assign": 2,
        "no-sequences": 2,
        "no-throw-literal": 2,
        "no-unmodified-loop-condition": 2,
        "no-unused-expressions": 2,
        "no-useless-call": 2,
        "no-useless-concat": 2,
        "no-with": 2,
        "radix": 2,
        "yoda": 2,
        "strict": 2,
        "no-catch-shadow": 2,

        "arrow-body-style": [2, "as-needed"],
        "arrow-parens": [2, "as-needed"],
        "arrow-spacing": 2,
        "constructor-super": 2,
        "no-confusing-arrow": 2,
        "no-useless-constructor": 2,
        "no-var": 2,
        "prefer-const": 2,
        "prefer-template": 2,
    }
}
